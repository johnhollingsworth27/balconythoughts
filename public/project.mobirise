value=\"@PROJECT_PATH@/assets/images/background13.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay&&bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div className\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div className\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div mbr-className\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div className\"row justify-content-center\" mbr-if=\"showTitle || showSubtitle\">\n            <div className\"col-12 col-md-{{textWidth}}\">\n                <h3 className\"mbr-section-title mb-0 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                    <b>Lunch &amp; Dinner</b></h3>\n                <h4 className\"mbr-section-subtitle mb-0 mt-2 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\">College of Information Science and Technology</h4>\n            </div>\n        </div>\n        <div className\"row justify-content-center mt-4\">\n            <div className\"col-12 col-md-{{textWidth}}\">\n                <ul className\"nav nav-tabs mb-4\" role=\"tablist\">\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item first mbr-fonts-style\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style show active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs1-2a_tab0\" data-app-placeholder=\"Type Text\" aria-selected=\"true\"><strong>Chicken Tikka Masala</strong></a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item\" mbr-if=\"bootstrapTabs > 1\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs1-2a_tab1\" data-app-placeholder=\"Type Text\" aria-selected=\"true\"><strong>Vodka Sauce</strong></a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item\" mbr-if=\"bootstrapTabs > 2\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs1-2a_tab2\" data-app-placeholder=\"Type Text\" aria-selected=\"true\"><strong>Chicken Teriyaki</strong></a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item\" mbr-if=\"bootstrapTabs > 3\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs1-2a_tab3\" data-app-placeholder=\"Type Text\" aria-selected=\"true\"><strong>Chipotle Chicken Tacos</strong></a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item\" mbr-if=\"bootstrapTabs > 4\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style active\" role=\"tab\" data-toggle=\"tab\" data-bs-toggle=\"tab\" href=\"#tabs1-2a_tab4\" data-app-placeholder=\"Type Text\" aria-selected=\"true\"><strong>Blackened Salmon</strong></a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" className\"nav-item\" mbr-if=\"bootstrapTabs > 5\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrLink,-mbrBtnRemove,-iconFont\"><a className\"nav-link mbr-fonts-style active\" role=\"tab\" data-tog